{
  "name": "Cursed Cursor",
  "description": "Ghost mode",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "panda.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["cursed_cursor.js"]
  }],
  "background": {
    "service_worker": "background.js"
  }
  ,
    "permissions": ["https://some-random-api.ml/"]
  
}